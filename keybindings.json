[
    {
        "key": "F1",
        "command": "editor.action.insertSnippet",
        "when": "editorLangId == latex",
        "args": {
            "snippet": "\\textbf{${TM_SELECTED_TEXT}$0}"
        }
    },
    {
        "key": "F2",
        "command": "editor.action.insertSnippet",
        "when": "editorLangId == latex",
        "args": {
            "snippet": "\\begin{itemize}\n\t\\item $0\n\\end{itemize}"
        }
    },
    {
        "key": "F3",
        "command": "editor.action.insertSnippet",
        "when": "editorLangId == latex",
        "args": {
            "snippet": "\\item ${1:$TM_SELECTED_TEXT}$0"
        }
    },
    {
        "key": "F4",
        "command": "editor.action.insertSnippet",
        "when": "editorLangId == latex",
        "args": {
            "snippet": "\\subsection*{${TM_SELECTED_TEXT}$0}"
        }
    },
    {
        "key": "F5",
        "command": "editor.action.insertSnippet",
        "when": "editorLangId == latex",
        "args": {
            "snippet": "\\centering\n\\huge\n\\textbf{${1:$TM_SELECTED_TEXT}}\n\\normalsize\n\\raggedright\n\\vspace{12pt}\n\\renewcommand{\\thesection}{\\arabic{section}}"
        }
    }
]
